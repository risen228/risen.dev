{
  "version": 2,
  "alias": "risen.dev",
  "builds": [
    {
      "use": "@now/static-build",
      "src": "package.json",
      "config": { "distDir": "public" }
    }
  ],
  "headers": [
    {
      "source": "/static/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/page-data/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    },
    {
      "source": "/(.*).(js|json|css|ico|png)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/cv",
      "destination": "https://risen.notion.site/Evgeny-Zakharov-d06d1c609ce8480a834f6349e19bf0df",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/123",
      "destination": "https://risen.notion.site/Evgeny-Zakharov-d06d1c609ce8480a834f6349e19bf0df/"
    },
    {
      "source": "/123(.*)",
      "destination": "https://risen.notion.site/Evgeny-Zakharov-d06d1c609ce8480a834f6349e19bf0df$1"
    }
  ]
}
