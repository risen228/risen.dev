{
  "version": 2,
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1",
      "headers": { "cache-control": "public, max-age=31536000, immutable" }
    },
    {
      "src": "/page-data/(.*)",
      "dest": "/page-data/$1",
      "headers": { "cache-control": "public, max-age=0, must-revalidate" }
    },
    {
      "src": "/(.*\\.(js|json|css|ico|png)$)",
      "dest": "/$1",
      "headers": { "cache-control": "public, max-age=31536000, immutable" }
    },
    {
      "src": "/(.*)",
      "dest": "/$1",
      "headers": { "cache-control": "public, max-age=0, must-revalidate" }
    }
  ],
  "alias": "risen.dev",
  "builds": [
    {
      "use": "@now/static-build",
      "src": "package.json",
      "config": { "distDir": "public" }
    }
  ],
  "redirects": [
    {
      "source": "/cv",
      "destination": "https://risen.notion.site/Evgeny-Zakharov-d06d1c609ce8480a834f6349e19bf0df",
      "permanent": true
    }
  ]
}
